@base <https://w3id.org/tso/BOTAlignment#> .

<https://w3id.org/tso/BOTAlignment#> rdf:type owl:Ontology ;
    owl:imports bot: , <https://w3id.org/tso/v0.2/tubes> ;
    dcterms:creator "Dominik Schl√ºtter" , "Nicolas Pauen" ;
    dcterms:description "This ontology defines correspondences with the BOT ontology. Alignment holds for BOT v0.3.2 and TSO v0.2." ;
    dcterms:title "TSO to BOT alignment"@en ;
    owl:versionInfo "v0.2" ;
    dcterms:issued "2021-02-21T15:00:00"^^xsd:dateTime ;
    dcterms:modified "2021-02-21T15:00:00"^^xsd:dateTime ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/bot#Element
bot:Element rdfs:subClassOf tso:System .

###  https://w3id.org/bot#Zone
bot:Zone owl:equivalentClass tso:Zone .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/bot#hasSubElement
bot:hasSubElement rdfs:subPropertyOf tso:hasSubSystem .


#################################################################
